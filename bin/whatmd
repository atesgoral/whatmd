#!/usr/bin/env node

var program  = require('commander'),
    fs = require('fs'),
    whatmd = require('../src/whatmd');

program
    .version('0.0.0')
    .usage('[filename]')
    .parse(process.argv);

var filename = program.args.shift();

if (filename) {
	var src = fs.readFileSync(filename);

    var findings = whatmd.probe(src);

	console.log(findings);
}
